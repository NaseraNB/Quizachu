<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/menu_fondo_color"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/botoLogin"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botoRegistre"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="37dp"
        android:background="@drawable/btn_main"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        android:text="@string/botoLogin" />

    <Button
        android:id="@+id/botoRegistre"
        android:layout_width="323dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="69dp"
        android:background="@drawable/btn_maindos"
        android:text="@string/botoRegistre"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="match_parent"
        android:layout_height="407dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="329dp"
        app:srcCompat="@drawable/menuselect" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="195dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="241dp"
        app:srcCompat="@drawable/imageestrella" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="78dp"
        android:layout_marginEnd="296dp"
        android:layout_marginBottom="703dp"
        app:srcCompat="@drawable/imageestrella" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="292dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="336dp"
        app:srcCompat="@drawable/imageestrella" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="333dp"
        android:layout_marginBottom="323dp"
        app:srcCompat="@drawable/imageestrella" />


</RelativeLayout>